<h1>Add Clients</h1>

<form action="/categories" method="post">
  <label for="new-category">Add a new Category</label>
  <input id="new-category" name="new-category" type="text">
  <button type="submit">Submit</button>
</form>
<ul>
@foreach(var categories in Model)
{
  <li>@categories.GetName()</li>
}
</ul>
<a href="/">Go Back</a><br>
<a href="/stylists">View All Stylists</a>
 
