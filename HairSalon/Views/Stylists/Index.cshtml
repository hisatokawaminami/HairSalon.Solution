<!DOCTYPE html>
<html>
  <head>
    <title>Stylists</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
  </head>
  <h1>Stylists</h1>
  <table class="table table-hover table-danger">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Phone Number</th>
    </tr>
  </thead>
  <tbody>

  @foreach(var stylists in Model)
  {
<tr>
      <td>@stylists.GetName()  |<a href="/stylists/@stylists.GetId()/clients">View the clients</a> | <a href="/stylists/@stylists.GetId()/edit">Edit</a>| <a href="/stylists/@stylists.GetId()/delete">Delete</a></td>
      <td>@stylists.GetPhone()  </td>




</tr>
  }

  </tbody>
</table>

<ul>


  <a href='/stylists/add'>Add a stylist</a><br>
  <a href='/specialties/add'>Add a specialty</a><br>
  <a href='/clients/add'>Add a client</a><br>
  <br><br>

  <a href="/stylists">View all stylist</a><br>
  <a href="/specialties">View all specialties</a><br>
  <a href="/clients">View all clients</a><br>
