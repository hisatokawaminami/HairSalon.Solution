<h1>Stylists</h1>
<h2>Edit Name: @Model.GetName()</h2>
<form action="/stylists/@Model.GetId()/edit" method="post">
    <label for="name">New Name</label>
    <input type="text" name="newname" value="@Model.GetName()">
    <button type="submit">Edit</button>
</form>
<h4><a href="/">Return home</a></h4>
